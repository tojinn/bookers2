
<%= render 'homes/list' %>
<%= render 'books/info' %>
<div class="col-md-8 offset-md-1">
 <h2>Users</h2>
 <table class="table">
  <thead>
   <tr>
    <th>image</th>
    <th>name</th>
    <th></th>
   </tr>
  </thead>
  
  <tbody>
   <% @users.each do |user| %>
   <tr>
    <td><%= attachment_image_tag user, :profile_image, size:'40x40', format: 'jpeg', fallback: "no_image.jpg" %></td>
    <td><%= user.name %></td>
    <td><%= link_to 'Show', user_path(user.id) %></td>
   </tr>
   <% end %>
  </tbody>
 </table>
</div>
</div>
<tbody>
 <tr></tr>
</tbody>
